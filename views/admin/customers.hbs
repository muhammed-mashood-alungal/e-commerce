 <section class="container">
    <table class="table mt-3" id="myTable">
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Total Purchase</th>
       
      <th>Options</th>
    </tr>
  </thead>
  <tbody>
    {{#each users}}
    <tr>
    
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.price}}</td> 
    
      <td> <a href="/admin/remove-user/{{this._id}}" onclick="return confirm('are you want delete {{this.name}}')"  class="btn btn-danger">Remove</a></td>
    </tr>

    {{/each}}
    
  </tbody>
</table>
 </section>